<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.welington.locedu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".View.ListaSetor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.Login"
            android:screenOrientation="portrait" />
        <activity android:name=".View.NovoSetor" />
        <activity android:name=".View.AlterarSetor" />
        <activity android:name=".View.ListaLocal" />
        <activity android:name=".View.NovoLocal" />
        <activity android:name=".View.AlterarLocal" />
        <activity android:name=".View.NovoEvento" />
        <activity android:name=".View.ListaEvento"></activity>
    </application>

</manifest>