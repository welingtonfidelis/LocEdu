<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.welingtonfidelis.locedu">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_INT" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:screenOrientation="portrait">
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.Login"/>
        <activity android:name="br.com.welingtonfidelis.locedu.View.ListaSetor" />
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.NovoSetor"
            android:theme="@style/AppTheme" />
        <activity android:name="br.com.welingtonfidelis.locedu.View.ListaLocal" />
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.NovoLocal"
            android:theme="@style/AppTheme" />
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.NovoEvento"
            android:theme="@style/AppTheme" />
        <activity android:name="br.com.welingtonfidelis.locedu.View.ListaEvento" />
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.PopUpInfoLocal"
            android:theme="@style/Theme.AppCompat.Dialog.MinWidth" />
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.PopUpInfoEvento"
            android:theme="@style/Theme.AppCompat.Dialog.MinWidth" />
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.PopUpFotoLocal"
            android:theme="@style/Theme.AppCompat.Dialog.MinWidth" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Base.Theme.AppCompat" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="" />

        <activity
            android:name="br.com.welingtonfidelis.locedu.View.Mapa"
            android:label="@string/title_activity_mapa"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.Feedback"
            android:theme="@style/Theme.AppCompat.Dialog.MinWidth" />
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.SplashScreen"
            android:theme="@style/AppTheme"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.PopUpListaMenu"
            android:theme="@style/Theme.AppCompat.Dialog.MinWidth" />
        <activity
            android:name="br.com.welingtonfidelis.locedu.View.Home"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
